   for user_bet in UserBet.get(UserBet.owner):
        print(user_bet)
        if (UserBet.get(UserBet.owner == user_bet).left_score > UserBet.get(UserBet.owner == user_bet).right_score) == left_win:  # czy trafiona strona, lewa
            score_for_user = (User
                              .update(score=+1)
                              .where(User.name == user_bet)
                              .execute())
            if (UserBet.get(UserBet.owner == user_bet).left_score + UserBet.get(UserBet.owner == user_bet).right_score) == left_win_value:  # czy trafiony dokładny wynik
                score_for_user = (User
                                  .update(score=+2)
                                  .where(User.name == user_bet)
                                  .execute())
        if (UserBet.get(UserBet.owner == user_bet).left_score < UserBet.get(UserBet.owner == user_bet).right_score) == right_win:  # czy trafiona strona, prawa
            score_for_user = (User
                              .update(score=+1)
                              .where(User.name == user_bet)
                              .execute())
            if UserBet.get(UserBet.owner == user_bet).left_score + UserBet.get(UserBet.owner == user_bet).right_score == right_win_value:  # czy trafiony dokładny wynik
                score_for_user = (User
                                  .update(score=+2)
                                  .where(User.name == user_bet)
                                  .execute())
        if (UserBet.get(UserBet.owner == user_bet).left_score == UserBet.get(UserBet.owner == user_bet).right_score) == draw:  # czy trafiony remis
            score_for_user = (User
                              .update(score=+1)
                              .where(User.name == user_bet)
                              .execute())
            if (UserBet.get(UserBet.owner == user_bet).left_score + UserBet.get(UserBet.owner == user_bet).right_score) == draw_value:  # czy trafiony dokładny wynik
                score_for_user = (User
                                  .update(score=+2)
                                  .where(User.name == user_bet)
                                  .execute())
